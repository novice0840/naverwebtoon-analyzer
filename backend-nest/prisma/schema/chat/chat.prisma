model Chat {
  id        String      @default(uuid()) @id
  content   String
  webtoon   Webtoon      @relation(fields: [webtoonId], references: [id])
  webtoonId String
  user      User        @relation(fields: [userId], references: [id])
  userId    String
  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
}